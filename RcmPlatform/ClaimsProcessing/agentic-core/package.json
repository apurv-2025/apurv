{
  "name": "@agentic/core",
  "version": "1.0.0",
  "description": "Reusable AI Agent Framework for intelligent automation",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "npm run build:backend && npm run build:frontend",
    "build:backend": "cd backend && python setup.py build",
    "build:frontend": "cd frontend && npm run build",
    "dev": "concurrently \"npm run dev:backend\" \"npm run dev:frontend\"",
    "dev:backend": "cd backend && uvicorn main:app --reload --port 8000",
    "dev:frontend": "cd frontend && npm start",
    "test": "npm run test:backend && npm run test:frontend",
    "test:backend": "cd backend && pytest",
    "test:frontend": "cd frontend && npm test",
    "lint": "npm run lint:backend && npm run lint:frontend",
    "lint:backend": "cd backend && flake8 .",
    "lint:frontend": "cd frontend && npm run lint",
    "format": "npm run format:backend && npm run format:frontend",
    "format:backend": "cd backend && black .",
    "format:frontend": "cd frontend && npm run format",
    "docker:build": "docker build -t agentic/core:latest .",
    "docker:run": "docker run -p 8000:8000 agentic/core:latest",
    "publish": "npm publish --access public"
  },
  "keywords": [
    "ai",
    "agent",
    "chat",
    "automation",
    "llm",
    "gpt",
    "claude",
    "react",
    "fastapi",
    "typescript"
  ],
  "author": "Agentic Core Team",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/agentic/core.git"
  },
  "bugs": {
    "url": "https://github.com/agentic/core/issues"
  },
  "homepage": "https://github.com/agentic/core#readme",
  "dependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "lucide-react": "^0.263.1",
    "axios": "^1.4.0",
    "clsx": "^2.0.0",
    "tailwind-merge": "^1.14.0"
  },
  "devDependencies": {
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "@types/node": "^20.0.0",
    "typescript": "^5.0.0",
    "tailwindcss": "^3.3.0",
    "autoprefixer": "^10.4.0",
    "postcss": "^8.4.0",
    "concurrently": "^8.0.0",
    "eslint": "^8.0.0",
    "prettier": "^3.0.0"
  },
  "peerDependencies": {
    "react": ">=18.0.0",
    "react-dom": ">=18.0.0"
  },
  "files": [
    "dist",
    "frontend/dist",
    "backend/dist",
    "README.md",
    "LICENSE"
  ],
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  }
} 